{
  "name": "pitch-sync",
  "description": "Webcomponent pitch-sync following open-wc recommendations",
  "license": "MIT",
  "author": "pitch-sync",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "analyze": "cem analyze --litelement",
    "preinstall": "npx npm-force-resolutions",
    "start": "concurrently \"npm run backend\" \"npm run wait-and-start-frontend\"",
    "backend": "node backend/server.js",
    "frontend": "web-dev-server",
    "wait-and-start-frontend": "wait-on http://localhost:5000/status && npm run frontend"
  },
  "resolutions": {
    "path-to-regexp": "8.0.0"
  },
  "dependencies": {
    "@vaadin/router": "^1.7.5",
    "bcryptjs": "^2.4.3",
    "concurrently": "^9.0.1",
    "jsonwebtoken": "^9.0.2",
    "lit": "^3.1.4"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.10.3",
    "@web/dev-server": "^0.4.6",
    "npm-force-resolutions": "^0.0.10",
    "wait-on": "^8.0.1"
  },
  "customElements": "custom-elements.json"
}
